<multiple name="objectlist">
	<div class="oneobject" @objectlist.onmouseover;noquote@>
		<a href="@objectlist.url;noquote@" class="objtitle">@objectlist.title;noquote@</a> <span class="objdetails"> (@objectlist.pretty_name@)</span>
		<div id="@objectlist.object_id@" class="objfulldetails"></div>
	</div>
</multiple>